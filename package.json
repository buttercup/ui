{
  "name": "@buttercup/ui",
  "version": "5.2.0",
  "description": "Buttercup UI Components in React",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:buttercup/buttercup-ui.git",
  "author": "Sallar Kaboli <sallar.kaboli@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && webpack --mode production --config webpack.config.js",
    "build:storybook": "build-storybook",
    "clean": "rimraf dist",
    "dev": "npm run clean && webpack --mode development --config webpack.config.js --progress --watch",
    "format": "prettier --write '{src,stories,__tests__}/**/*.js'",
    "prepublishOnly": "npm run build",
    "storybook": "start-storybook -p 6006",
    "test": "run-s test:unit test:format",
    "test:format": "prettier-check '{src,stories,__tests__}/**/*.js'",
    "test:unit": "jest",
    "test:updateSnapshots": "jest -u"
  },
  "files": [
    "dist/**/*"
  ],
  "lint-staged": {
    "{src,stories,__tests__}/**/*.js": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif)$": "<rootDir>__tests__/mocks/file.mock.js",
      "\\.(scss|sass|css|less)$": "<rootDir>__tests__/mocks/file.mock.js"
    },
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/index.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testRegex": "/__tests__/.*\\.spec\\.js$"
  },
  "resolutions": {
    "@storybook/{app}/webpack": "^5"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-function-bind": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-transform-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addons": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.19",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.16",
    "@types/react-splitter-layout": "^3.0.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "arraybuffer-loader": "^1.0.8",
    "babel-loader": "^8.2.3",
    "babel-plugin-jsx-control-statements": "^4.1.2",
    "babel-plugin-ramda": "^2.0.0",
    "buttercup": "^6.13.1",
    "css-loader": "^6.7.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "extract-domain": "^2.2.1",
    "fast-deep-equal": "^2.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.2.5",
    "i18next": "^19.9.1",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "jsx-control-statements": "^3.2.8",
    "lint-staged": "^10.1.3",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path-posix": "^1.0.0",
    "prettier": "^1.19.1",
    "prettier-check": "^2.0.0",
    "random-words": "^1.1.2",
    "react": "^17.0.2",
    "react-credit-cards": "^0.8.3",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.0.2",
    "react-hotkeys": "^1.1.4",
    "react-splitter-layout": "^4.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.5",
    "terser-webpack-plugin": "^5.3.1",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "xbytes": "^1.7.0"
  },
  "peerDependencies": {
    "buttercup": ">=6",
    "react": ">=16",
    "react-dom": ">=16",
    "styled-components": ">=5"
  },
  "dependencies": {
    "@blueprintjs/core": "^4.3.0",
    "@blueprintjs/icons": "^4.2.5",
    "@buttercup/generator": "^2.0.0",
    "@buttercup/react-formatted-input": "^1.4.0",
    "@fontsource/inter": "^4.1.0",
    "classnames": "^2.2.6",
    "filter-react-dom-props": "^0.0.2",
    "fuse.js": "^3.4.5",
    "otpauth": "^7.1.2",
    "polished": "^1.9.3",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.0",
    "react-icons": "^4.3.1",
    "react-popover": "^0.5.10",
    "react-textarea-autosize": "^7.1.2",
    "zxcvbn": "^4.4.2"
  }
}
